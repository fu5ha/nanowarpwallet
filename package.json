  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "buffer": "5.0.8",
    "colors": "1.1.2",
    "iced-coffee-script": "108.0.11",
    "iced-error": "0.0.12",
    "iced-runtime": "1.0.1",
    "iced-test": "0.0.23",
    "iced-utils": "0.1.26",
    "icsify": "0.7.0",
    "parcel-bundler": "termhn/parcel#deterministic",
    "qrcode-generator": "^1.3.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
